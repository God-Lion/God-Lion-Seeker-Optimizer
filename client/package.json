  "main": "./src/index.tsx",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:check": "tsc && vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx vite-bundle-visualizer",
    "analyze:stats": "vite build --mode analyze && npx vite-bundle-visualizer",
    "lighthouse": "lighthouse http://localhost:3000 --view --output html --output-path ./lighthouse-report.html",
    "lighthouse:ci": "lhci autorun",
    "perf:build": "npm run build && npm run analyze",
    "perf:profile": "node --inspect-brk ./node_modules/.bin/vite build",
    "size": "npm run build && size-limit",
    "test:perf": "npm run build && npm run lighthouse"
  },
